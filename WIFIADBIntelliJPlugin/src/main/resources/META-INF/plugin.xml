<?xml version="1.0" encoding="UTF-8"?>
<idea-plugin version="2">
    <id>adb.wifi.woaiwhz</id>
    <name>WifiAdbIntellijPlugin</name>
    <version>0.0.1</version>
    <vendor email="huazhou19@gmail.com" url="https://github.com/Sausure/WIFIADB/tree/master/WIFIADBIntelliJPlugin">
        HuaZhou Wang - (Sausure)
    </vendor>

    <description><![CDATA[
      Sample plugin.<br>
    ]]></description>

    <change-notes><![CDATA[
      Release 0.0.1: Initial release.<br>
    ]]>
    </change-notes>

    <!-- please see https://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
    <idea-version since-build="131"/>

    <!-- please see https://confluence.jetbrains.com/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <defaultProjectTypeProvider type="Android"/>
        <toolWindow
                id="WiFi Adb Plugin"
                icon="/icons/icon.png"
                anchor="right"
                factoryClass="adb.wifi.woaiwhz.window.RootWindow"
                />
    </extensions>

    <application-components>
    </application-components>

    <project-components>
    </project-components>

    <depends>org.jetbrains.android</depends>

    <actions>
    </actions>

</idea-plugin>